          fusionJWPlayer.comscore('adstart');
        });

        var amznAdsResponse_refresh = function() { if ('amznads' in window && 'getTargeting' in window.amznads) {var amznTargs = ''; var targeting = amznads.getTargeting(); if (targeting) {for (var key in targeting) {var val = targeting[key]; if (val instanceof Array) {val = val.join(',');} amznTargs += encodeURIComponent(key + '=' + val + '&');}} videoTargs = '%26' + amznTargs; }};

