(function ($) {

  $( document ).ready(function() {
    $('a').click(function(e) {
      //e.preventDefault();
      var category = '';
      var action = '';
      var opt_label = '';
      var opt_value = '';
      var opt_noninteractive = '';
      var index = 1;
      var type = 'Link';

      if ($(this).closest('[data-gtm-category]').attr('data-gtm-category') &&
        $(this).closest('[data-gtm-action]').attr('data-gtm-action') &&
        this.href) {

        // Initialized required variables
        category = $(this).closest('[data-gtm-category]').attr('data-gtm-category');
        action = $(this).closest('[data-gtm-action]').attr('data-gtm-action');
        opt_label = this.href;
        opt_label_parts = opt_label.split('-');

        // Capture more accurate index number
        link_parent_tag = $(this).parent().prop('tagName');
        link_parent_class = $(this).parent().attr('class');
        link_parent_class = link_parent_class ? '.' + link_parent_class.replace(' ','.') : '';
        links = $(this).closest('[data-gtm-action]').find(link_parent_tag + ' ' + link_parent_class + ' a');

        // Find index of link
        if (links) {
          $.each( links, function( key, value ) {
            if (value.href == opt_label) {
              index = key + 1;
            }
          });
        }

        // Find type of link
        if ($(this).find('img').attr('src')) {
          type = 'Image';
        }
        else if (opt_label_parts && Number(opt_label_parts.pop())) {
          type = 'Title';
        }
        else if (opt_label.indexOf('reporters/') >= 0) {
          type = 'Author';
        }

        // Combine action with type and index
        if (type) {
          action += '_' + type + '_' + index;
        }

        //console.log('category: ' + category + ', action: ' + action + ', opt_label: ' + opt_label + ', opt_value: ' + opt_value);

        if (category && action && opt_label) {
          analyticsEvent(category,action,opt_label,opt_value,opt_noninteractive);
        }
      }


    });

    analyticsEvent = function(category,action,opt_label,opt_value,opt_noninteractive) {
      dataLayer.push({
        'event': 'analyticsEvent',
        'eventCategory': category,
        'eventAction': action,
        'eventLabel': opt_label,
        'eventValue': opt_value,
        'eventNonInt': true
      });
    }

    /**
     * Initializes jw player player
     */
    Drupal.behaviors.gtm = {
      initialized: false,
  /*
      attach: function(context, settings) {
        var e = this;
        if (settings.fusion_jwplayer.init == true) {
          gtm.init();
        }
      },

      init: function() {
        if (this.initialized) {
          return;
        }
      },

  */

    };
  });
})(jQuery);
